{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "localServerStorage": {
    "name": "localServerStorage",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "server/csvFiles",
    "nameConflict": "makeUnique",
    "maxFileSize": "52428800"
  },
  "transactional": {
    "host": "localhost",
    "port": 27017,
    "database": "gamechange",
    "name": "transactional",
    "connector": "mongodb",
    "lazyConnect": true,
    "autoReconnect": true,
    "reconnectTries": 50,
    "reconnectInterval": 1000,
    "keepAlive": true,
    "connectionTimeout": 300000,
    "socketTimeout": 300000
  }
}
